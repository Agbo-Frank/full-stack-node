<script type="module">
  import { faker } from 'https://esm.sh/@faker-js/faker';

  const getEvents = (type) => {
    const name = faker.person.firstName();
    const amount = faker.number.int({min: 500, max: 20000}).toLocaleString("en-US");
    const country = faker.location.country()

    return {
      'withdraw': { title: "New Withdrawal", subtitle: `${name} withdrawal request of $${amount} has been processed` },
      'signup': { title: "New signup", subtitle: `${name} from ${country} just signed up`},
      'deposit': { title: "New Deposit", subtitle: `${name} has deposited $${amount} into their account.` },
      // 'investment': { title: "Withdrawal", subtitle: `John Doe has made a new investment of $5,000.` },
    }[type]
  }

  $(window).on('load', function () {
   const randomName = faker.person.firstName();
   const events = ["withdraw", 'signup', 'investment', 'deposit']
  
   setInterval(() => {
      const index = faker.number.int({min: 0, max: 3});
      const { title, subtitle } = getEvents(events[index])
      tata.text(title, subtitle, { position: "br", progress: true, duration: 3000, animate: "slide" });
    }, 25000) 
  });

 

</script>
