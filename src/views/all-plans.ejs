<%- include('partials/head') %>
<%- include('partials/header') %>
<%- include('partials/banner', {name: "Plans"}) %>

<!-- Dashboard Section Starts Here -->
<div class="dashbaord-section padding-bottom">
  <div class="container">
    <div class="row">
      <%- include('partials/sidebar', {active: "admin/plans"}) %>
      <div class="col-lg-8 col-xl-8 col-xxl-9">
        <div class="dashboard__wrapper">
            <div class="dashboard__header">
                <div class="inner">
                  <h3 class="title">Plans</h3>
                </div>
                <div class="inner">
                  <ul class="tabs d-flex justify-content-end flex-wrap align-items-end">
                    <li>
                      <div class="user-thumb">
                        <img src="assets/images/dashboard/cc1.png" alt="dashboard">
                      </div>
                    </li>
                  </ul>
                </div>
            </div>
            <div class="dashboard__table__container">
              <table style="width: 100%;" class="table mt-5 table-responsive transection__table dashboard__table">
                <thead>
                    <tr>
                      <th>Name</th>
                      <th>Rate</th>
                      <th>Max</th>
                      <th>Min</th>
                    </tr>
                </thead>
                <tbody>
                  <% data.forEach(i => { %>
                    <tr data-plan="<%= Object.entries(i).map(i => `${i[0]}:${i[1]}`).join(',') %>" class="search--btn plan__details__btn">
                      <td data-label="Plan Name">
                        <p class="name"><%= i?.name %></p>
                      </td>
                      <td data-label="Rate"><span class="amount"><%= i?.rate %>%</span></td>
                      <td data-label="Max price"><span class="amount"><%= i?.max_price %></span></td>
                      <td data-label="Min price"><span class="amount"><%= i?.min_price %></span></td>
                    </tr>
                  <% }) %>
                </tbody>
              </table>
            </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- Dashboard Section Ends Here -->

<%- include('partials/footer') %>
<%- include('partials/scripts') %>